{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/script.js",
            "dest": "/script.js"
        },
        {
            "src": "/",
            "dest": "/index.html"
        }
    ]
}
